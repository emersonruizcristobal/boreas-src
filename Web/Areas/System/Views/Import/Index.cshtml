@model Web.Areas.Shared.ViewModels.SystemImportIndexViewModel
@{
    ViewBag.Title = "Products";
    Layout = "~/Areas/Shared/Views/_Layout.cshtml";
}

@section Styles{
    <link href="~/Content/css/application/system/import.index.css" rel="stylesheet">
}

<div class="dv-upper-button align-button pull-right" role="toolbar">
    <div class="row">
        @Html.Partial("~/Areas/Shared/Views/ImportCSV.cshtml", Model.ImportCSVViewModel)
    </div>
</div>
<h1 class="title badge">Import Products</h1>

<div class="form-group">
    <div class="table-responsive top-spacer-xs">
        <table id="tbl-users" class="table table-striped table-hover">
            <thead>
                <tr class="head-text">
                    <th>Name</th>
                    <th>Description</th>
                    <th>UOM</th>
                    <th>Bin</th>
                    <th>SBU</th>
                    <th>Date Imported</th>
                </tr>
            </thead>
            <tbody id="tdy-users">
                
            </tbody>
        </table>
    </div>
</div>