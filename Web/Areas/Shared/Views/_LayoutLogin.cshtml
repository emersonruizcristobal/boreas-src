@{
    var settingService = new Service.Setting.SettingService();
    var version = "1.0";
    try {
        version = typeof(Web.MvcApplication).Assembly.GetName().Version.ToString();
    } catch (Exception exception) {

    }
}

<!DOCTYPE html>
<html lang="en">
<head>
   
    <title>Konstruksiyon - @ViewBag.Title</title>
    <link href="~/content/lib/amaran/amaran.css" rel="stylesheet">
    <link href="~/Content/lib/sbadmin/sb-admin-2.css" rel="stylesheet" type="text/css">

    @RenderSection("Styles", required: false)

    <script>
        if (location.protocol != 'https:' && (location.hostname != "localhost")) {
            location.href = 'https:' + window.location.href.substring(window.location.protocol.length);
        }
    </script>

</head>

<body>
    @RenderBody()
    <!-- Bootstrap core JavaScript-->
    <script src="~/Content/lib/jquery/jquery.min.js"></script>
    <script src="~/Content/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- Core plugin JavaScript-->
    <script src="~/Content/lib/easing/easing.min.js"></script>
    <!-- Custom scripts for all pages-->
    <script src="~/Content/lib/sbadmin/sb-admin-2.min.js"></script>
    <script src="~/Content/lib/amaran/amaran.js"></script>
    <script src="~/Content/lib/underscore/underscore.js"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>

